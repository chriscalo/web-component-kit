{
  "name": "@chriscalo/web-component-kit",
  "version": "1.0.0",
  "description": "A reactive template binding system and web component toolkit for rapid HTML prototyping",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "node --test --no-warnings index.test.js",
    "test:pw": "playwright test",
    "test:headed": "playwright test --headed",
    "test:ui": "playwright test --ui",
    "start": "npx http-server -p 3000 -c-1 -o"
  },
  "keywords": [
    "web-components",
    "template-bindings",
    "reactive",
    "prototyping",
    "browser",
    "zero-build"
  ],
  "author": "Chris Calo",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/chriscalo/web-component-kit.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "dependencies": {
    "@vue/reactivity": "^3.4.21"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.1",
    "http-server": "^14.1.1",
    "playwright": "^1.49.0"
  }
}
